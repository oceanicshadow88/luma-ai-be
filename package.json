{
  "name": "luma-ai-be",
  "version": "1.0.0",
  "description": "Express.js and TypeScript backend for LUMA AI",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/loaders/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --ext ts --exec ts-node ./loaders/server.ts",
    "debug": "node --inspect dist/loaders/server.js",
    "lint": "eslint \"{src,loaders,tests}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,tests,loaders}/**/*.ts\"",
    "lint:check": "eslint . --ext .ts",
    "format:check": "prettier --check \"{src,tests,loaders}/**/*.ts\"",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "check": "yarn format:check && yarn lint:check && yarn test && echo 'All checks passed!'",
    "lint-staged:run": "lint-staged",
    "spell": "cspell \"loaders/**/*.ts\" \"src/**/*.ts\" \"tests/**/*.ts\" \"*.ts\" \"**/*.{md,json}\"",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.{ts,tsx,json,md}": "cspell"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-requests-logger": "^4.0.3",
    "express-validator": "^7.2.1",
    "freemail": "^1.7.0",
    "fs-extra": "11.2.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "minimist": "1.2.8",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.14.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3",
    "psl": "^1.15.0",
    "redis": "^5.0.1",
    "supertest": "^7.1.0",
    "winston": "^3.17.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@sonar/scan": "^4.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.3",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cspell": "^9.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
